mkdir /data/comments
cd /data/comments
for (( i=0; i<256; i++)); do
strl=`printf "%02X" $i`
mkdir $strl
cd $strl
for (( j=0; j<256; j++)); do
strc=`printf "%02X" $j`
mkdir $strc
done
cd ..
done
cd ..


