<include file="public:header_min" />
<div class="user_head_bg">
	<div class="user_head">
		<div class="logobox">
			<a href="__ROOT__/"><img src="<if condition="C('qscms_logo_home')">{:attach(C('qscms_logo_home'),'resource')}<else/>../public/images/logo.gif</if>" border="0"/></a>
		</div>
		<div class="logotxt">
			|&nbsp;&nbsp;
			<if condition="ACTION_NAME eq 'login'">
				LOGIN
			<else/>
				<if condition="$utype eq 0">REGISTER</if>
				<if condition="$utype eq 1">COMPANY</if>
				<if condition="$utype eq 2">PERSONAL</if>
			</if>
		</div>
		<div class="reg">
			<if condition="ACTION_NAME eq 'login'">
				Unregistered? <a href="{:U('members/register')}" class="btn_blue J_hoverbut btn_inline">Register Now</a>
			<else/>
				Registered? <a href="{:U('members/login')}" class="btn_blue J_hoverbut btn_inline">Login Now</a>
			</if>
		</div>
		<div class="clear"></div>
	</div>
</div>