/*bottomBox*/
.bottomBox{
    margin-top:445px;
    min-width: 1200px;
    background-color: #F2F2F2;
}
.bottomBox .bottom{
    width:1200px;
    margin:0 auto;
    border-top: 2px solid #FF4720;
}
.bottomBox .bottom .bottomLink{
    font-size: 13px;
    height:38px;
    line-height:38px;
    text-align: center;
    /*padding-left: 40px;*/
}
.bottomBox .bottom .bottomLink a{
    color: #666;
    padding: 0 12px;
}
.bottomBox .bottom .bottomCopyRight{
    height:35px;
    line-height:35px;
}
.bottomBox .bottom .bottomCopyRight .bottomCopyRightText{
    margin:0 22px;
    border-top:1px dashed #666;
    text-align: center;
}
.bottomBox .bottom .bottomCopyRight .bottomCopyRightText .copy{
    font-size: 12px;
    color: #999;
}
.bottomBox .bottom .bottomCopyRight .bottomCopyRightText .copy .copyLink{
    color: #999;
    text-decoration: underline;
}
.bottomBox .bottom .bottomCopyRight .bottomCopyRightText .contact{
    font-size: 13px;
    color: #3366FF;
    text-decoration: underline;
}