<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<include file="public:meta" />
	<link href="../public/css/common.css" rel="stylesheet" type="text/css" />
	<link href="../public/css/common_ajax_dialog.css" rel="stylesheet" type="text/css" />
	<link href="../public/css/jobs.css" rel="stylesheet" type="text/css" />
	<script src="../public/js/jquery.common.js" type="text/javascript" language="javascript"></script>
	<qscms:load type="category" search="1"/>
	<if condition="C('SUBSITE_VAL.s_id') gt 0 and !$_GET['citycategory']">
		<qscms:classify 列表名="city" 类型="QS_citycategory" 地区分类="$subsite_val['s_district']" 显示数目="100"/>
	<else/>
		<qscms:classify 列表名="city" 类型="QS_citycategory" 地区分类="$_GET['citycategory']" 显示数目="100"/>
	</if>  
</head>
<body>
<include file="public:header" />
<!-- search for  -->
<div class="page_so">
	<div class="sobox">
		<div class="selecttype">
			<if condition="C('qscms_jobsearch_key_first_choice') eq 1">
				<div class="J_sli sli <if condition="$Think.get.search_type eq 'jobs' or $Think.get.search_type eq ''">select</if>" data-type="jobs"> Search posts </div>
			<elseif condition="C('qscms_jobsearch_type') neq 0"/>
				<div class="J_sli sli <if condition="$Think.get.search_type eq 'full' or $Think.get.search_type eq ''">select</if>" data-type="full"> All </div>
			</if>
			<if condition="C('qscms_jobsearch_key_first_choice') eq 0 && C('qscms_jobsearch_type') eq 0">
				<div class="J_sli sli <if condition="$Think.get.search_type eq 'jobs' or $Think.get.search_type eq ''">select</if>" data-type="jobs"> Search posts </div>
			</if>
			<div class="J_sli sli <if condition="$Think.get.search_type eq 'company'">select</if>" data-type="company"> Search Company </div>
			<if condition="C('qscms_jobsearch_type') neq 0 && C('qscms_jobsearch_key_first_choice') eq 1">
				<div class="J_sli sli <if condition="$Think.get.search_type eq 'full'">select</if>" data-type="full"> full text </div>
			</if>
			<div class="clear"></div>
		</div>
		<div class="inputbg">
			<div id="showSearchModal" title="" class="selecttype J_hoverbut" data-title=" please select the region " data-multiple="false" data-maxnum="0" data-width="520"> Select area </div>
			<form id="ajax_search_location" action="{:U('ajaxCommon/ajax_search_location',array('type'=>'QS_jobslist'))}" method="get">
				<div class="inoputbox">
					<input type="text" name="key" data-original="{$Think.get.key|urldecode|urldecode}" value="{$Think.get.key|urldecode|urldecode}" placeholder=" Please enter key words " />
					<input type="hidden" name="search_type" value="{$Think.get.search_type}" />
					<input id="searchCityModalCode" type="hidden" name="citycategory" value="{$city['select']['citycategory']}" />
					<input id="recoverSearchCityModalCode" type="hidden" name="" value="" />
					<input type="hidden" name="jobcategory" value="{$Think.get.jobcategory}" />
					<input class="J_forclear" type="hidden" name="jobtag" value="{$Think.get.jobtag}" />
					<input class="J_forclear" type="hidden" name="wage" value="{$Think.get.wage}" />
	        		<input class="J_forclear" type="hidden" name="trade" value="{$Think.get.trade}" />
	        		<input class="J_forclear" type="hidden" name="scale" value="{$Think.get.scale}" />
	        		<input class="J_forclear" type="hidden" name="nature" value="{$Think.get.nature}" />
	        		<input class="J_forclear" type="hidden" name="education" value="{$Think.get.education}" />
	        		<input class="J_forclear" type="hidden" name="experience" value="{$Think.get.experience}" />
	        		<input class="J_forclear" type="hidden" name="settr" value="{$Think.get.settr}" />
                    <input type="hidden" name="lng" id="lng"  value="{$Think.get.lng}"/>
                    <input type="hidden" name="lat" id="lat"  value="{$Think.get.lat}"/>
				</div>
				<input type="submit" class="sobut J_hoverbut" value=" search for " />
			</form>
		</div>
  		<div class="righttxt">
            <div class="sr-map-left link_gray6"><a href="{:url_rewrite('QS_jobs')}">分类 search for </a></div>
            <div class="sr-map-right link_gray6">
                <span class="cur-map-pos" title=""></span>
                <a href="javascript:;" id="popupBox"> Map looking for work </a>
                <a class="map-clear" href="{:url_rewrite('QS_jobslist')}"> Cleared </a>
            </div>
            <div class="clear"></div>
        </div>
		<div class="clear"></div>
	</div>
    <div class="hotword link_gray9 font_gray9 nowrap">
    	 Popular keywords ：
    	<qscms:hotword 列表名="hotword_list" 显示数目="5"/>
    	<volist name="hotword_list" id="hotword">
			<if condition="C('qscms_key_urlencode') eq 1">
			<a href="{:url_rewrite('QS_jobslist',array('key'=>urlencode($hotword['w_word_code']),'search_type'=>$_GET['search_type']))}">{$hotword.w_word}</a>
			<else />
			<a href="{:url_rewrite('QS_jobslist',array('key'=>$hotword['w_word_code'],'search_type'=>$_GET['search_type']))}">{$hotword.w_word}</a>
			</if>
    	</volist>
    </div>
	<!--条件 -->
	<div class="so_condition_show">
		<div class="J_showbtn showbtn unfold none"> Open the filter </div> 
		<div class="J_showbtn showbtn"> Put away the filter </div>
		<div class="clear"></div>
	</div>
</div>
<div class="so_condition J_so_condition">
	<qscms:classify 列表名="jobsCate" 类型="QS_jobs"/>
	<qscms:classify 列表名="jobs_cate_info" 类型="QS_jobs_info"/>
	<if condition="C('qscms_jobsearch_key_open_jobcategory') eq 1">
		<div class="lefttit"> Job classification </div>
		<if condition="$Think.get.jobcategory eq '' or C('qscms_category_jobs_level') eq 2">
			<div class="rs">
				<div class="J_jobConditions">
					<div class="wli <empty name="Think.get.jobcategory">select</empty>"><a href="{:P(array('jobcategory'=>''))}"> All </a></div>
					<volist name="jobsCate[0]" id="scate">
						<volist name="jobsCate[$key]" id="cate">
							<div class="wli <eq name="cate['spell']" value="$Think.get.jobcategory">select</eq>"><a href="{:P(array('jobcategory'=>$cate['spell']))}">{$cate.categoryname}</a></div>
						</volist>
					</volist>
					<div class="clear"></div>
				</div>
			</div>
			<div class="more">
				<div class="J_showJobConditions mbtn close"> Put away </div>
				<div class="J_showJobConditions mbtn none"> More </div>
			</div>
		<else/>
			<div class="rs_classify">
				<div class="wli_y" onclick="window.location='{:P(array('jobcategory'=>''))}';">
					<notempty name="jobsCate[$jobs_cate_info['spell'][$_GET['jobcategory']]['id']]">
						{$jobs_cate_info['spell'][$_GET['jobcategory']]['categoryname']}
						<assign name="category" value="$jobs_cate_info['spell'][$_GET['jobcategory']]['id']"/>
					<else/>
						{$jobs_cate_info['id'][$jobs_cate_info['spell'][$_GET['jobcategory']]['parentid']]['categoryname']}
						<assign name="category" value="$jobs_cate_info['spell'][$_GET['jobcategory']]['parentid']"/>
					</notempty>
				</div>
				<div class="clear"></div>
		        <div class="showclassify">
					<div class="toparrow"></div>
					<div class="wli <eq name="jobs_cate_info['spell'][$_GET['jobcategory']]['id']" value="$category">select</eq>">
						<a href="{:P(array('jobcategory'=>$jobs_cate_info['id'][$category]['spell']))}"> All </a>
					</div>
					<volist name="jobsCate[$category]" id="cate">
						<div class="wli <eq name="cate['spell']" value="$Think.get.jobcategory">select</eq>">
							<a href="{:P(array('jobcategory'=>$cate['spell']))}">{$cate.categoryname}</a>
						</div>
					</volist>
					<div class="clear"></div>
				</div>
			</div>
		</if>
		<div class="clear"></div>
	</if>
	<if condition="$Think.get.citycategory neq ''">
		<notempty name="city['list']">
			<div class="lefttit"> Landmarks </div>
			<div class="rs">
				<div class="li <if condition="$city['parent']['id'] eq $city['select']['id']">select</if>"><a href="{:P(array('citycategory'=>$city['parent']['citycategory']))}"> Not limited to </a></div>
				<volist name="city['list']" id="district">
					<div class="li <if condition="$city['select']['id'] eq $key">select</if>"><a href="{:P(array('citycategory'=>$district['citycategory']))}">{$district['categoryname']}</a></div>
				</volist>
				<div class="clear"></div>
			</div>
			<div class="clear"></div>
		</notempty>
	</if>
	<div class="lefttit"> Job salary </div>
	<div class="rs">
		<qscms:classify 列表名="wage_list" 类型="QS_wage" 显示数目="100"/>
		<div class="li <if condition="$Think.get.wage eq ''">select</if>"><a href="{:P(array('wage'=>''))}"> Not limited to </a></div>
		<volist name="wage_list" id="wage">
			<div class="li <if condition="$Think.get.wage eq $key">select</if>"><a href="{:P(array('wage'=>$key))}">{$wage}</a></div>
		</volist>
		<div class="clear"></div>
	</div>
	<div class="clear"></div>
	<div class="lefttit"> Position highlights </div>
	<div class="rs">
		<qscms:classify 列表名="tag_list" 类型="QS_jobtag" 显示数目="100"/>
		<div class="li <if condition="$Think.get.jobtag eq ''">select</if>"><a href="{:P(array('jobtag'=>''))}"> Not limited to </a></div>
		<volist name="tag_list" id="tag">
			<div class="li <if condition="$Think.get.jobtag eq $key">select</if>"><a href="{:P(array('jobtag'=>$key))}">{$tag}</a></div>
		</volist>
		<div class="clear"></div>
	</div>
	<div class="clear"></div>
  
	<div class="lefttit"> More 筛选</div>
	<div class="rs">
		<div class="bli J_dropdown">
			<qscms:classify 列表名="trade_list" 类型="QS_trade" 显示数目="100"/>
			<span class="txt" title="{$trade_list[$_GET['trade']]|default=' Industry '}">{$trade_list[$_GET['trade']]|default=' Industry '}</span>
			<div class="dropdowbox2 J_dropdown_menu">
	            <div class="dropdow_inner2">
	                <ul class="nav_box">
	                	<volist name="trade_list" id="trade">
				            <li><a class="<if condition="$Think.get.trade eq $key">select</if>" href="{:P(array('trade'=>$key))}" data-code="{$key}">{$trade}</a></li>
						</volist>
						<div class="clear"></div>
	                </ul>
	            </div>
	        </div>
			<div class="clear"></div>
		</div>
		<div class="bli J_dropdown">
			<span> Enterprise scale </span>
			<qscms:classify 列表名="scale_list" 类型="QS_scale" 显示数目="100"/>
			<div class="dropdowbox1 J_dropdown_menu">
	            <div class="dropdow_inner1">
	                <ul class="nav_box">
	                	<volist name="scale_list" id="scale">
				            <li><a class="<if condition="$Think.get.scale eq $key">select</if>" href="{:P(array('scale'=>$key))}" data-code="{$key}">{$scale}</a></li>
						</volist>
	                </ul>
	            </div>
	        </div>
	        <div class="clear"></div>
		</div>
		<div class="bli J_dropdown">
			<span> Nature of the work </span>
			<qscms:classify 列表名="nature_list" 类型="QS_jobs_nature" 显示数目="100"/>
			<div class="dropdowbox1 J_dropdown_menu">
	            <div class="dropdow_inner1">
	                <ul class="nav_box">
	                	<volist name="nature_list" id="nature">
				            <li><a class="<if condition="$Think.get.nature eq $key">select</if>" href="{:P(array('nature'=>$key))}" data-code="{$key}">{$nature}</a></li>
						</volist>
	                </ul>
	            </div>
	        </div>
			<div class="clear"></div>
		</div>
		<div class="bli J_dropdown">
			<span> Academic requirements </span>
			<qscms:classify 列表名="education_list" 类型="QS_education" 显示数目="100"/>
			<div class="dropdowbox1 J_dropdown_menu">
	            <div class="dropdow_inner1">
	                <ul class="nav_box">
	                	<volist name="education_list" id="education">
				            <li><a class="<if condition="$Think.get.education eq $key">select</if>" href="{:P(array('education'=>$key))}" data-code="{$key}">{$education}</a></li>
						</volist>
	                </ul>
	            </div>
	        </div>
			<div class="clear"></div>
		</div>
		<div class="bli J_dropdown">
			<span> work experience </span>
			<qscms:classify 列表名="experience_list" 类型="QS_experience" 显示数目="100"/>
			<div class="dropdowbox1 J_dropdown_menu">
	            <div class="dropdow_inner1">
	                <ul class="nav_box">
	                	<volist name="experience_list" id="experience">
				            <li><a class="<if condition="$Think.get.experience eq $key">select</if>" href="{:P(array('experience'=>$key))}" data-code="{$key}">{$experience}</a></li>
						</volist>
	                </ul>
	            </div>
	        </div>
			<div class="clear"></div>
		</div>
		<div class="bli J_dropdown">
			<span> Update time </span>
			<div class="dropdowbox1 J_dropdown_menu">
	            <div class="dropdow_inner1">
	                <ul class="nav_box">
	                	<li><a class="<if condition="$Think.get.settr eq 3">select</if>" href="{:P(array('settr'=>3))}" data-code="3">3 Days </a></li>
	                	<li><a class="<if condition="$Think.get.settr eq 7">select</if>" href="{:P(array('settr'=>7))}" data-code="7">7 Days </a></li>
	                	<li><a class="<if condition="$Think.get.settr eq 15">select</if>" href="{:P(array('settr'=>15))}" data-code="15">15 Days </a></li>
	                	<li><a class="<if condition="$Think.get.settr eq 30">select</if>" href="{:P(array('settr'=>30))}" data-code="30">30 Days </a></li>
	                </ul>
	            </div>
	        </div>
			<div class="clear"></div>
		</div>
		<div class="clear"></div>
	</div>
	<div class="clear"></div>

	<!-- Selected condition ，当没有条件的时候不显示下面的DIV -->
	<if condition="$_GET">
		<div class="selected J_selected">
			<div class="stit"> Selected condition </div>
		    <div class="sc">
		    	<notempty name="Think.get.key">
					<div class="slist" <if condition="$Think.get.sort eq 'score'">onclick="window.location='{:P(array('key'=>'','sort'=>''))}';"<else/>onclick="window.location='{:P(array('key'=>''))}';"</if>><span> Keywords ：</span>{$Think.get.key|urldecode|urldecode}</div>
		    	</notempty>
				<if condition="$Think.get.jobcategory neq ''">
					<div class="slist" onclick="window.location='{:P(array('jobcategory'=>''))}';">
						<span> Job classification ：</span>
						{$jobs_cate_info['spell'][$_GET['jobcategory']]['categoryname']}
					</div>
				</if>
				<if condition="$Think.get.citycategory neq ''">
					<div class="slist" onclick="window.location='{:P(array('citycategory'=>''))}';">
						<span> Area category ：</span>
						{$city['select']['categoryname']}
					</div>
				</if>
				<if condition="$Think.get.trade neq ''">
					<div class="slist" onclick="window.location='{:P(array('trade'=>''))}';">
						<span> Industry ：</span>
						{$trade_list[$_GET['trade']]}
					</div>
				</if>
				<if condition="$Think.get.jobtag neq ''">
					<div class="slist" onclick="window.location='{:P(array('jobtag'=>''))}';"><span> Position highlights ：</span>{$tag_list[$_GET['jobtag']]}</div>
				</if>
				<if condition="$Think.get.wage neq ''">
					<div class="slist" onclick="window.location='{:P(array('wage'=>''))}';"><span> Job salary ：</span>{$wage_list[$_GET['wage']]}</div>
				</if>
				<if condition="$Think.get.education neq ''">
					<div class="slist" onclick="window.location='{:P(array('education'=>''))}';"><span> Academic requirements ：</span>{$education_list[$_GET['education']]}</div>
				</if>
				<if condition="$Think.get.experience neq ''">
					<div class="slist" onclick="window.location='{:P(array('experience'=>''))}';"><span> work experience ：</span>{$experience_list[$_GET['experience']]}</div>
				</if>
				<if condition="$Think.get.settr neq ''">
					<div class="slist" onclick="window.location='{:P(array('settr'=>''))}';">
						<span> Update time ：</span>
						<switch name="_GET['settr']">
							<case value="3">3 Days </case>
							<case value="7">7 Days </case>
							<case value="15">15 Days </case>
							<case value="30">30 Days </case>
						</switch>
					</div>
				</if>
				<if condition="$Think.get.nature neq ''">
					<div class="slist" onclick="window.location='{:P(array('nature'=>''))}';"><span> Nature of the work ：</span>{$nature_list[$_GET['nature']]}</div>
				</if>
				<if condition="$Think.get.scale neq ''">
					<div class="slist" onclick="window.location='{:P(array('scale'=>''))}';"><span> Enterprise scale ：</span>{$scale_list[$_GET['scale']]}</div>
				</if>
				<div class="clear"></div>
			</div>
			<div class="sr">
				<div class="empty" onclick="window.location='{:url_rewrite('QS_jobslist')}';"> Emptied </div>
			</div>
			<div class="clear"></div>
		</div>
	</if>
</div>
<qscms:jobs_list 列表名="jobslist" 搜索类型="$_GET['search_type']" 搜索内容="$_GET['search_cont']" 显示数目="20" 分页显示="1" 关键字="$_GET['key']" 职位分类="$_GET['jobcategory']" 地区分类="$_GET['citycategory']" 行业="$_GET['trade']" 日期范围="$_GET['settr']" 学历="$_GET['education']" 工作经验="$_GET['experience']" 工资="$_GET['wage']" 职位性质="$_GET['nature']" 标签="$_GET['jobtag']" 公司规模="$_GET['scale']" 营业执照="$_GET['license']" 过滤已投递="$_GET['deliver']" 排序="$_GET['sort']" 合并="$_COOKIE['com_list']" 描述长度="100"/>
<div class="plist">
	<div class="pl">	
		<div class="toptitle">
			<div class="ltype">
				<div class="typeli <if condition="$Think.get.search_cont eq ''">select</if>" onclick="window.location='{:P(array('search_cont'=>''))}';"> All positions </div>
				<div class="typeli <if condition="$Think.get.search_cont eq 'setmeal'">select</if>" onclick="window.location='{:P(array('search_cont'=>'setmeal'))}';"> Recruitment of enterprises </div>
				<if condition="C('apply.Sincerity')">
				<div class="typeli <if condition="$Think.get.search_cont eq 'famous'">select</if>" onclick="window.location='{:P(array('search_cont'=>'famous'))}';"> Recruitment </div>
				</if>
				<div class="clear"></div>
			</div>
			<div class="ts">
		  		<div class="l1"></div>
					<div class="l2 <if condition="$Think.get.deliver eq 1">select</if>">
					<if condition="$visitor['utype'] eq 2">
						<div class="radio_group" <if condition="$Think.get.deliver eq 1">onclick="window.location='{:P(array('deliver'=>''))}'"<else/>onclick="window.location='{:P(array('deliver'=>1))}'"</if>>
							<div class="radiobox"></div>
							<div class="radiotxt"> Filter has been delivered </div>
							<div class="clear"></div>
						</div>
					</if>
			  </div>			
				<div class="l2 <if condition="$Think.get.license eq 1">select</if>">
					<div class="radio_group" <if condition="$Think.get.license eq 1">onclick="window.location='{:P(array('license'=>''))}';"<else/>onclick="window.location='{:P(array('license'=>1))}';"</if>>
						<div class="radiobox"></div>
						<div class="radiotxt"> Business license is certified </div>
						<div class="clear"></div>
					</div>
				</div>
		
				<div class="J_detailList l3 <if condition="$Think.cookie.jobs_show_type eq ''">select</if>" title=" Switch to the detailed list "></div>
				<div class="J_detailList l4 <if condition="$Think.cookie.jobs_show_type eq 1">select</if>" title=" Switch to simple list " show_type="1"></div>
				<div class="l5">
					<if condition="$jobslist['page_params']['nowPage'] gt 1">
				  		<div class="prev" title=" Previous page " onclick="window.location='{:P(array('p'=>$jobslist['page_params']['nowPage']-1))}';"><</div>
				  	</if>
				  	<if condition="$jobslist['page_params']['nowPage'] lt $jobslist['page_params']['totalPages']">
						<div class="next"  title=" Next page " onclick="window.location='{:P(array('p'=>$jobslist['page_params']['nowPage']+1))}';">></div>
					</if>
					<if condition="$jobslist['page_params']['totalRows'] gt 0">
						<span>{$jobslist.page_params.nowPage}</span>/{$jobslist.page_params.totalPages} page 
					</if>
					<div class="clear"></div>
				</div>
				<div class="clear"></div>
			</div>
			<div class="sort">
				<div class="sl1"> Sort by ：</div>
				<if condition="C('qscms_jobsearch_type') neq 0 and $Think.get.search_type eq 'full' and $Think.get.key neq ''">
					<a class="sl2 <if condition="$Think.get.sort eq 'score' or ($Think.get.sort eq '' and C('qscms_fulltext_orderby') eq 1)">select</if>" href="{:P(array('sort'=>'score'))}"> relativity </a>
				<else/>
					<a class="sl2 <if condition="$Think.get.sort eq ''">select</if>" href="{:P(array('sort'=>''))}"> Integrated sorting </a>
				</if>
				<a class="sl2 <if condition="$Think.get.sort eq 'rtime' or ($Think.get.sort eq '' and C('qscms_jobsearch_type') neq 0 and $Think.get.search_type eq 'full' and C('qscms_fulltext_orderby') eq 0)">select</if>" href="{:P(array('sort'=>'rtime'))}"> Update time </a>
				<div class="clear"></div>
			</div>
		</div>
		<!--列表 -->
		<div class="listb J_allListBox">
			<notempty name="jobslist['list']">
				<volist name="jobslist['list']" id="jobs">
					<div class="J_jobsList yli" data-jid="{$jobs.id}">
