<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <include file="public:meta"/>
    <link href="../public/css/personal/common.css" rel="stylesheet" type="text/css"/>
    <link href="../public/css/personal/personal_job.css" rel="stylesheet" type="text/css"/>
    <link href="../public/css/personal/personal_ajax_dialog.css" rel="stylesheet" type="text/css"/>
    <script src="../public/js/personal/jquery.common.js" type="text/javascript" language="javascript"></script>
</head>
<body>
<include file="index_header"/>
<div class="user_main">
    <include file="index_left_nav"/>
    <div class="mainbox">
        <!--职位切换卡 -->
        <div class="tab">
            <a class="li J_hoverbut" href="{:U('jobs_interview')}"> Interview Invitation </a>
            <a class="li J_hoverbut" href="{:U('jobs_apply')}"> Have applied for a job </a>
            <a class="li J_hoverbut" href="{:U('attention_me')}"> Who is paying attention to me </a>
            <a class="li select"> Visited the post </a>
            <div class="clear"></div>
        </div>
        <!--切换卡结束 -->

        <div class="my_attention_th">
            <div class="th1"> Job Title </div>
            <div class="th2"> company name </div>
            <div class="th3"> Position status </div>
            <div class="th4">
                <div class="input_100_30_div J_hoverinput J_dropdown J_listitme_parent">
                    <span class="J_listitme_text"><if condition="$settr eq 0"> Browse time <else/>{$settr} Days </if></span>
                    <div class="dropdowbox7 J_dropdown_menu">
                        <div class="dropdow_inner7">
                            <ul class="nav_box">
                                <li><a class="J_listitme" href="{:P(array('settr'=>0))}"> All </a></li>
                                <li><a class="J_listitme" href="{:P(array('settr'=>3))}">3 Days </a></li>
                                <li><a class="J_listitme" href="{:P(array('settr'=>7))}">7 Days </a></li>
                                <li><a class="J_listitme" href="{:P(array('settr'=>15))}">15 Days </a></li>
                                <li><a class="J_listitme" href="{:P(array('settr'=>30))}">30 Days </a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="th5"> operating </div>
            <div class="clear"></div>
        </div>
        <notempty name="jobs_list['list']">
            <form id="form1" action="{:U('del_view_jobs')}" method="post" class="J_allListBox">
                <volist name="jobs_list['list']" id="vo">
                    <div class="my_attention J_hoverbut">
                        <div class="td1 link_blue substring"><input class="J_allList" name="y_id[]" type="checkbox"
                                                          value="{$vo['id']}"/> &nbsp;<a
                                href="{$vo['url']}" target="_blank" title="{$vo['jobs_name']}">{$vo['jobs_name']}</a></div>
                        <div class="td2 link_gray6 substring"><a href="{$vo['company_url']}"
                                                       target="_blank" title="{$vo['companyname']}">{$vo['companyname']}</a></div>
                        <if condition="$vo['clase'] eq '1'">
                            <div class="td3 cl2"> closed </div>
                            <elseif condition="$vo['status'] eq '1'"/>
                            <div class="td3"> announcing </div>
                            <else/>
                            <div class="td3 cl1">{$vo['status_cn']}</div>
                        </if>
                        <div class="td4">{$vo['addtime']|date="Y-m-d",###}</div>
                        <div class="td5">
                            <a href="javascript:;" url="{:U('del_view_jobs',array('y_id'=>$vo['id']))}"
                               class="but2 J_hoverbut delete"></a>
                            <div class="clear"></div>
                        </div>
                        <div class="clear"></div>
                    </div>
                </volist>
                <div class="my_attention_but">
                    <div class="td1"><input name="" class="J_allSelected" type="checkbox" value=""/></div>

                    <div class="td3">
                        <div class="but_gray_80_32 J_hoverbut delall"> delete </div>
                    </div>
                    <div class="clear"></div>
                </div>
            </form>
            <div class="qspage">{$jobs_list['page']}</div>
            <else/>
            <if condition="$hasget">
                <div class="res_empty">
                     Sorry ， No records found to match your criteria ， We recommend that you review the filter criteria and try again 
                </div>
                <else/>
                <div class="res_empty link_blue">
                     You have not seen a job yet ， I suggest you take the initiative to find a job ！<br/>
                     Massive high salary positions you choose 。 immediately  <a href="{:url_rewrite('QS_jobs')}" target="_blank"> Search for jobs </a>
                </div>
            </if>
        </notempty>
    </div>
    <div class="clear"></div>
</div>
<include file="index_footer"/>
<script type="text/javascript" src="../public/js/jquery.listitem.js"></script>
<script type="text/javascript" src="../public/js/jquery.dropdown.js"></script>
<script type="text/javascript" src="../public/js/jquery.allselected.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $(".delall").click(function () {
            var listCheckedArray = $('.J_allListBox .J_allList:checked');
            if (listCheckedArray.length) {
                var url = $("#form1").attr('action');
                var qsDialog = $(this).dialog({
                    title: ' delete  Visited the post ',
                    loading: true,
                    border: false,
                    yes: function () {
                        $("#form1").submit();
                    }
                });
                $.getJSON(url, function (result) {
                    if (result.status == 1) {
                        qsDialog.setContent(result.data.html);
                    } else {
                        disapperTooltip('remind', result.msg);
                    }
                });
            } else {
                disapperTooltip("remind", "请选择要 delete 的数据");
            }
        });
        $(".delete").click(function () {
            var url = $(this).attr('url');
            var qsDialog = $(this).dialog({
                title: ' delete  Visited the post ',
                loading: true,
                border: false,
                yes: function () {
                    window.location.href = url;
                }
            });
            $.getJSON(url, function (result) {
                if (result.status == 1) {
                    qsDialog.setContent(result.data.html);
                } else {
                    disapperTooltip('remind', result.msg);
                }
            });
        });
    });
</script>
</body>
</html>
