@charset "utf-8";a,b,body,button,dd,div,dl,dt,em,footer,h1,h2,h3,h4,h5,header,img,input,li,nav,ol,option,p,span,strong,table,td,th,tr,ul,var{margin:0;padding:0;outline:0;-webkit-touch-callout:none;-webkit-user-select:text;-moz-user-select:text}html{font-size:16px}@font-face{font-family:Regular;src:url(../font/DIN-Regular.otf)}@font-face{font-family:Bold;src:url(../font/DIN-Bold.otf)}body{width:100%;overflow-x:hidden;word-break:break-word;font-family:Regular,'.PingFang SC','Helvetica Neue',Helvetica,Verdana,Arial,sans-serif;-webkit-tap-highlight-color:rgba(0,0,0,0)}table{border-collapse:collapse;border-spacing:0}h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:400}li,ol,ul{list-style:none}em,i{font-style:normal}fieldset,img{border:none}img,input{vertical-align:middle}span{display:inline-block}article,aside,details,figcaption,figure,footer,header,hgroup,nav,section,summary{display:block}a{background:0 0;cursor:pointer}a:active,a:hover{outline:0;text-decoration:none}a:link,a:visited,ins{text-decoration:none}button,textarea{font-size:100%;-webkit-appearance:none}.clear{clear:both;height:0;overflow:hidden}.hidden{display:none}.container,.row{padding:0}.line-1px{width:100%;height:1px;border-bottom:1px solid #d9d9d9;-webkit-transform:scaleY(0.5);-ms-transform:scaleY(0.5);transform:scaleY(0.5);-webkit-transform-origin:0 0;-ms-transform-origin:0 0;transform-origin:0 0}@media all and (max-width:1200px){html{font-size:18px}}@media all and (max-width:395px){html{font-size:16px}}@media all and (max-width:360px){html{font-size:15px}}@media all and (max-width:330px){html{font-size:14px}}

/*首行缩进*/
input,textarea{
    padding-left: 0.3125rem !important;
}

/*commonPopBox1弹窗*/
.commonPopBox1 {
    position: absolute;
    background-color: rgba(0, 0, 0, 0.6);
    z-index: 9999;
    width: 100%;
    height: 100%;
}
.commonPopBox1 .commonPop {
    width: 20.9375rem;
    background-color: #fff;
    margin: 0 auto;
    margin-top: 9.375rem;
}
.commonPopBox1 .commonPop .title {
    /*height: 2.3125rem;*/
    /*line-height: 2.3125rem;*/
    padding: 0.625rem 0;
    padding-left: 0.75rem;
    font-size: 0.875rem;
    background-color: #cccccc;
}
.commonPopBox1 .commonPop .detailTip {
    text-align: justify;
    padding: 2rem 2.5rem;
    font-size: 0.875rem;
}
.commonPopBox1 .commonPop .confirmBox {
    font-size: 0.8125rem;
    padding: 0.75rem 2.5rem;
    text-align: center;
}
.commonPopBox1 .commonPop .confirmBox .confirmBtn {
    cursor: pointer;
    width: 7.9375rem;
    /*height: 2.1875rem;*/
    /*line-height: 2.1875rem;*/
    padding: 0.5rem 0;
    background-color: #ff4f0d;
    color: #fff;
    text-align: center;
}
