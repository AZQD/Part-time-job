/*headerBox*/
body {
    width: 100%;
}
.headerBox {
    height: 2.5rem;
    /*line-height: 2.5rem;*/
    background-color: #DCDCDC;
    position: fixed;
    z-index: 9;
}
.headerBox .left .logo {
    width: 100%;
    padding: 0 0.375rem;
    margin-bottom: 0.4rem;
    margin-top: 0.5625rem;
}
.headerBox .left .sort {
    /*width: 100%;*/
    width:1.4rem;
    margin: 0 0.375rem;
    padding-bottom: 0.2rem;
    margin-top: 0.4rem;
}
.headerBox .left .shopping {
    /*width: 100%;*/
    width:1.8rem;
    margin: 0 0.375rem;
    padding-bottom: 0.2rem;
}

.headerBox .indexMiddle {
    text-align: center;
    position: relative;
    height: 2.5rem;
}
.headerBox .manageMiddle, .headerBox .profileMiddle, .headerBox .publishMiddle, .headerBox .preSalesMiddle{
    font-size: 1rem;
    color: #333333;
    text-align: center;
    font-weight: bold;
    padding-top: 0.4375rem;
}
.headerBox .indexMiddle .address {
    color: #333333;
    font-size: 1.125rem;
}
.headerBox .indexMiddle .address:after {
    display: inline-block;
    content: "";
    border-top: 0.45rem solid #000000;
    border-right: 0.4rem solid transparent;
    border-bottom: 0.4rem solid transparent;
    border-left: 0.4rem solid transparent;
    position: relative;
    top: 0.325rem;
    left: 0.125rem;
}
.headerBox .indexMiddle .address2{
    /*padding-left:3.4rem;*/
}
.headerBox .indexMiddle .address2:after{
    display: none;
}
.headerBox .indexMiddle .addressUl {
    position: absolute;
    width: 100%;
    text-align: center;
    z-index: 2;
    top: 2.5rem;
}
.headerBox .indexMiddle .addressUl .addressLi {
    margin: 0 auto;
    width: 6.375rem;
    /*height: 1.875rem;*/
    /*line-height: 1.875rem;*/
    padding: 0.3125rem 0;
    font-size: 0.8125rem;
    color: #333333;
    border: 1px solid #999999;
    border-bottom: none;
    background-color: #fff;
}
.headerBox .indexMiddle .addressUl .addressLi:last-child {
    border-bottom: 1px solid #cccccc;
}

.headerBox .right {
    text-align: right;
    padding-right: 0.125rem;
}
.headerBox .right .search {
    width: 3rem;
    /*vertical-align: -0.625rem;*/
    position: relative;
    top:-0.45rem;
}
.headerBox .right .logout {
    text-decoration: underline;
    padding-right: 0.375rem;
}