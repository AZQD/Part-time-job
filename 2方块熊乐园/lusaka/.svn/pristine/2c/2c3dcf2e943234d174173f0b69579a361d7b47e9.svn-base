/*toggleSortBox*/
.toggleSortBox {
  margin-bottom: 0.3125rem;
}
.toggleSortBox .toggleSort {
  padding: 0.25rem 0;
  border-top: 0.125rem solid #ff4720;
  background-color: #DCDCDC;
}
.toggleSortBox .toggleSort .tab {
  text-align: center;
  border-right: 1px solid #333;
}
.toggleSortBox .toggleSort .tab:last-child {
  border-right: none;
}
.toggleSortBox .toggleSort .tab .text {
  font-size: 0.875rem;
  color: #333333;
  font-weight: bold;
}
.toggleSortBox .toggleSort .tab .text:after {
  display: inline-block;
  content: "";
  border-top: 0.5rem solid #000000;
  border-right: 0.375rem solid transparent;
  border-bottom: 0.375rem solid transparent;
  border-left: 0.375rem solid transparent;
  position: relative;
  top: 0.375rem;
  left: 0.125rem;
}
.toggleSortBox .toggleList {
  position: relative;
}
.toggleSortBox .toggleList .uls {
  position: absolute;
  top: 0;
  z-index: 9;
  text-align: center;
  border-bottom: 1px solid #999999;
}
.toggleSortBox .toggleList .uls li {
  padding: 0.3125rem 0;
  font-size: 0.8125rem;
  color: #333333;
  border: 1px solid #999999;
  border-bottom: none;
  background-color: #fff;
}
.toggleSortBox .toggleList .priceUl {
  left: 0;
}
.toggleSortBox .toggleList .qualityUl {
  left: 33.33333%;
}
.toggleSortBox .toggleList .areaUl {
  left: 66.66666%;
}
/*selectBox*/
.selectBox .select {
  margin: 0 auto;
  padding: 1.125rem 0;
  line-height: 2rem;
}
.selectBox .select .selectPrice {
  padding-left: 1rem;
}
.selectBox .select .selectPrice .price {
  font-size: 1rem;
  color: #333333;
  font-weight: bold;
  display: inline-block;
  text-align: right;
}
.selectBox .select .selectPrice a {
  padding-left: 1.25rem;
  color: #999;
}
.selectBox .select .selectPrice .active {
  color: #FF4720;
}
.selectBox .select .selectQuality {
  padding-left: 1rem;
}
.selectBox .select .selectQuality .quality {
  font-size: 1rem;
  color: #333333;
  font-weight: bold;
  display: inline-block;
  text-align: center;
}
.selectBox .select .selectQuality a {
  padding-left: 1rem;
  color: #999;
}
.selectBox .select .selectQuality .active {
  color: #FF4720;
}
.selectBox .select .selectArea {
  padding-left: 1rem;
}
.selectBox .select .selectArea .area {
  font-size: 1rem;
  color: #333333;
  font-weight: bold;
  display: inline-block;
  text-align: right;
  margin-right: 0.25rem;
}
.selectBox .select .selectArea a {
  padding-left: 1.125rem;
  color: #999;
}
.selectBox .select .selectArea .active {
  color: #FF4720;
}
/*tabsBox*/
.tabsBox {
  background-color: #F2F2F2;
}
.tabsBox .tabs {
  margin: 0 auto;
  border-bottom: 0.125rem solid #ff4720;
}
.tabsBox .tabs .tabsLeft {
  font-size: 0;
}
.tabsBox .tabs .tabsLeft .tabLink {
  display: inline-block;
  width: 3.5rem;
  height: 1.75rem;
  margin-top: 0.625rem;
  padding-top: 0.375rem;
  text-align: center;
  font-size: 0.8125rem;
  color: #333;
  border-radius: 0.3125rem 0.3125rem 0 0;
}
.tabsBox .tabs .tabsLeft .active {
  background-color: #FF4720;
  color: #fff;
}
.tabsBox .tabs .tabsRight {
  text-align: right;
}
.tabsBox .tabs .tabsRight .tabBtn {
  width: 3.375rem;
  height: 1.5rem;
  line-height: 1.5625rem;
  color: #333;
  font-size: 0.8125rem;
  outline: none;
  background-color: #ffffff;
  margin-top: 0.625rem;
  border: 1px solid #333;
}
.tabsBox .tabs .tabsRight .tabBtn1 {
  border-radius: 0.3125rem 0 0 0.3125rem;
}
.tabsBox .tabs .tabsRight .tabBtn1Up:before {
  display: inline-block;
  content: "";
  border-top: 0.5rem solid #000000;
  border-right: 0.375rem solid transparent;
  border-bottom: 0.375rem solid transparent;
  border-left: 0.375rem solid transparent;
  position: relative;
  top: 0.3125rem;
}
.tabsBox .tabs .tabsRight .tabBtn1Down:before {
  display: inline-block;
  content: "";
  border-top: 0.375rem solid transparent;
  border-right: 0.375rem solid transparent;
  border-bottom: 0.5rem solid #000000;
  border-left: 0.375rem solid transparent;
  position: relative;
  top: 0rem;
}
.tabsBox .tabs .tabsRight .tabBtn2 {
  margin-right: 0.625rem;
  border-radius: 0 0.3125rem 0.3125rem 0;
  border-left: 0;
}
.tabsBox .tabs .tabsRight .tabBtn2Up:before {
  display: inline-block;
  content: "";
  border-top: 0.5rem solid #000000;
  border-right: 0.375rem solid transparent;
  border-bottom: 0.375rem solid transparent;
  border-left: 0.375rem solid transparent;
  position: relative;
  top: 0.3125rem;
}
.tabsBox .tabs .tabsRight .tabBtn2Down:before {
  display: inline-block;
  content: "";
  border-top: 0.375rem solid transparent;
  border-right: 0.375rem solid transparent;
  border-bottom: 0.5rem solid #000000;
  border-left: 0.375rem solid transparent;
  position: relative;
  top: 0rem;
}
/*goodsBox*/
.goodsBox {
  margin-bottom: 3.75rem;
}
.goodsBox .goods {
  cursor: pointer;
  margin: 0 auto;
  /*background-color: #baf;*/
  padding: 0.625rem;
  border-bottom: 1px solid #666;
}
.goodsBox .goods .goodsLeft {
  width: 5rem;
  height: 5rem;
  position: relative;
  margin-right: 0.375rem;
}
.goodsBox .goods .goodsLeft img {
  width: 100%;
  height: 100%;
}
.goodsBox .goods .goodsLeft .tip {
  display: inline-block;
  width: 1.4375rem;
  height: 1.125rem;
  line-height: 1.125rem;
  color: #ffffff;
  font-size: 0.8125rem;
  text-align: center;
  background-color: #FF4720;
  position: absolute;
  right: 0;
  bottom: 0;
}
.goodsMiddle {
  text-align: left;
}
.goodsBox .goods .goodsMiddle .goodName {
  font-size: 1rem;
  color: #3366FF;
  font-weight: bold;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.goodsBox .goods .goodsMiddle .goodDesc {
  font-size: 0.875rem;
  color: #999;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.goodsBox .goods .goodsMiddle .goodDate {
  color: #333;
}
.goodsBox .goods .goodsRight {
  font-weight: bold;
  text-align: center;
  padding-top: 1.125rem;
}
.goodsBox .goods .goodsRight1 {
  font-size: 0.8125rem;
  color: #999999;
}
.goodsBox .goods .goodsRight2 {
  font-size: 1rem;
  color: red;
}
.goodsBox .goods .aboutQuality {
  font-size: 1rem;
  color: red;
}
#footerIframe {
  width: 100%;
  margin-bottom: -0.3125rem;
  background-color: #F2F2F2;
}
/*pageControl*/
.pageControl {
  text-align: center;
  padding: 0.625rem 0;
  margin-bottom: 3.125rem;
}
.pageControl .pageBtn {
  outline: none;
  border: none;
  min-width: 4rem;
  height: 1.5rem;
  line-height: 1.5rem;
  border-radius: 0.25rem;
  color: #333;
  font-size: 0.875rem;
  font-weight: bold;
  background-color: #FF4720;
}
#dongHua {
  display: none;
  position: absolute;
  width: 100%;
  height: 100%;
  /*background-color: RGBA(0, 0, 0, 0.6);*/
  /*background-color: #000000;*/
  /*opacity: .6;*/
  color: #f00;
  line-height: 100%;
  text-align: center;
  z-index: 999999999;
}
#dongHua img {
  width: 3.125rem;
  height: 3.125rem;
}
/*infos*/
.infos {
  position: fixed;
  width: 100%;
  left: 0;
  bottom: 0;
  box-shadow: 0 0 1.25rem #333333;
}
.infos .part {
  font-size: 0.875rem;
  color: #fff;
  text-align: center;
  padding: 0.625rem 0;
  background-color: #FF4720;
  font-weight: bold;
}
.infos .categoryPublish {
  background: #ff4720 url("../image/index/borderWhite.png") no-repeat right top;
  background-size: 1px 100%;
}
.infos .part span {
  text-decoration: underline;
}
/*产品类别展示*/
/*classifyBox*/
.classifyBoxWrap {
  position: fixed;
  top: 2.5rem;
  left: 0;
  box-shadow: 0 0.125rem 0.375rem #999999;
  background-color: #fff;
}
.classifyBox {
  background-color: #fff;
  position: relative;
  left: 0.3125rem;
}
.classifyBox .classifyLi .firstName {
  text-align: right;
  padding-right: 0.9375rem;
  height: 2rem;
  line-height: 2rem;
  font-size: 0.875rem;
  color: #333;
  font-weight: bold;
}
.classifyBox .classifyLi .secondNames {
  padding-left: 1.125rem;
  font-size: 0.875rem;
  color: #666;
  border-left: 1px solid #999;
}
.classifyBox .classifyLi .secondNames .secondName {
  height: 2rem;
  line-height: 2rem;
  padding-right: 0.625rem;
  text-decoration: underline;
}
