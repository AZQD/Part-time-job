/*tabsBox*/
.tabsBox{
  //min-width: 1200/@rem;
  background-color: #F2F2F2;
}
.tabsBox .tabs{
  //width:1200/@rem;
  margin:0 auto;
  border-bottom: 2/@rem solid #FF4720;
}
.tabsBox .tabs .tabsLeft{
  font-size: 0;
}
.tabsBox .tabs .tabsLeft .tabLink{
  display: inline-block;
  width: 56/@rem;
  height: 28/@rem;
  //line-height: 28/@rem;
  margin-top: 10/@rem;
  padding-top: 6/@rem;
  text-align: center;
  font-size: 13/@rem;
  color: #333;
  border-radius: 5/@rem 5/@rem 0 0;
}
.tabsBox .tabs .tabsLeft .active{
  background-color: #FF4720;
  color: #fff;
}
.tabsBox .tabs .tabsRight{
  text-align: right;
}
.tabsBox .tabs .tabsRight .tabBtn{
  width: 54/@rem;
  height: 24/@rem;
  line-height:25/@rem;
  color: #333;
  font-size: 13/@rem;
  outline: none;
  background-color: #ffffff;
  margin-top:10/@rem;
  border: 1px solid #333;
}
.tabsBox .tabs .tabsRight .tabBtn1{
  border-radius: 5/@rem 0 0 5/@rem;
}
.tabsBox .tabs .tabsRight .tabBtn1Up:before{
  display: inline-block;
  content: "";
  border-top: 8/@rem solid black;
  border-right: 6/@rem solid transparent;
  border-bottom: 6/@rem solid transparent;
  border-left: 6/@rem solid transparent;
  position: relative;
  top: 5/@rem;
}
.tabsBox .tabs .tabsRight .tabBtn1Down:before{
  display: inline-block;
  content: "";
  border-top: 6/@rem solid transparent;
  border-right: 6/@rem solid transparent;
  border-bottom: 8/@rem solid black;
  border-left: 6/@rem solid transparent;
  position: relative;
  top: 0/@rem;
}
.tabsBox .tabs .tabsRight .tabBtn2{
  margin-right: 10/@rem;
  border-radius: 0 5/@rem 5/@rem 0;
  border-left:0;
}
.tabsBox .tabs .tabsRight .tabBtn2Up:before{
  display: inline-block;
  content: "";
  border-top: 8/@rem solid black;
  border-right: 6/@rem solid transparent;
  border-bottom: 6/@rem solid transparent;
  border-left: 6/@rem solid transparent;
  position: relative;
  top: 5/@rem;
}
.tabsBox .tabs .tabsRight .tabBtn2Down:before{
  display: inline-block;
  content: "";
  border-top: 6/@rem solid transparent;
  border-right: 6/@rem solid transparent;
  border-bottom: 8/@rem solid black;
  border-left: 6/@rem solid transparent;
  position: relative;
  top: 0/@rem;
}

/*goodsBox*/
.goodsBox{
  //margin-top:10/@rem;
  margin-bottom: 60/@rem;
}
.goodsBox .goods{
  cursor: pointer;
  //width:1200/@rem;
  margin:0 auto;
  /*background-color: #baf;*/
  padding:10/@rem;
  border-bottom: 1px solid #666;
}
.goodsBox .goods .goodsLeft{
  width: 80/@rem;
  height: 80/@rem;
  position: relative;
  margin-right:6/@rem;
}
.goodsBox .goods .goodsLeft img{
  width:100%;
  height:100%;
}
.goodsBox .goods .goodsLeft .tip{
  display: inline-block;
  width: 23/@rem;
  height: 18/@rem;
  line-height:18/@rem;
  color: #ffffff;
  font-size: 13/@rem;
  text-align: center;
  background-color: #FF4720;
  position: absolute;
  right:0;
  bottom:0;
}
.goodsMiddle{
  text-align: left;
}
.goodsBox .goods .goodsMiddle .goodName{
  font-size: 16/@rem;
  color: #3366FF;
  font-weight:bold;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.goodsBox .goods .goodsMiddle .goodDesc{
  font-size: 14/@rem;
  color: #999;
  //padding-top:10/@rem;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.goodsBox .goods .goodsMiddle .goodDate{
  color: #333;
}
.goodsBox .goods .goodsRight{
  //height: 100/@rem;
  //line-height: 100/@rem;
  font-weight: bold;
  text-align: center;
  //margin-left:40/@rem;
  padding-top: 18/@rem;
}
.goodsBox .goods .goodsRight1{
  font-size: 13/@rem;
  color: #999999;
}
.goodsBox .goods .goodsRight2{
  font-size: 16/@rem;
  color: red;
  //padding-right: 40/@rem;
}
.goodsBox .goods .aboutQuality{
  font-size: 16/@rem;
  color: red;
  //min-width: 100/@rem;
}

