/*login_registerBox*/
.loginRegisterBox {
  margin-top: 5.9375rem;
}
.loginRegisterBox .loginRegister {
  margin: 0 auto;
}
.loginRegisterBox .loginRegister .loginWrap .loginBox {
  width: 18.125rem;
  margin: 0 auto;
  position: relative;
  /*background-color: #bfa;*/
}
.loginRegisterBox .loginRegister .loginWrap .loginBox .warning {
  width: 12.3125rem;
  height: 5.0625rem;
  position: absolute;
  right: -12.875rem;
  top: 6.8125rem;
  z-index: 999;
  color: #666;
  font-size: 0.8125rem;
  padding: 0.5rem 1rem;
  line-height: 1rem;
  background-color: #B3CCFF;
}
.loginRegisterBox .loginRegister .loginWrap .loginBox .warning .leftTop {
  width: 0.625rem;
  height: 1.875rem;
  background-color: #B3CCFF;
  position: absolute;
  left: -0.5rem;
  top: 0;
}
.loginRegisterBox .loginRegister .loginWrap .loginBox .loginTitle {
  font-size: 1.125rem;
  color: #333;
  font-weight: bold;
  margin: 1rem 0;
}
.loginRegisterBox .loginRegister .loginWrap .loginBox .loginTitle .gang {
  color: #FF4720;
  padding-left: 0.3125rem;
}
.loginRegisterBox .loginRegister .loginPhone {
  height: 2.625rem;
  margin-bottom: 0.625rem;
}
.loginRegisterBox .loginRegister .loginPhone .phoneImg {
  width: 2.625rem;
  height: 2.625rem;
}
.loginRegisterBox .loginRegister .loginPhone .phoneText {
  width: 15.5rem;
  height: 2.625rem;
  line-height: 2.625rem;
  outline: none;
  border: 1px solid #999;
  text-indent: 0.3125rem;
}
.loginRegisterBox .loginRegister .verifyCode {
  height: 1.875rem;
  line-height: 1.875rem;
  margin-bottom: 0.625rem;
}
.loginRegisterBox .loginRegister .verifyCode .phoneText {
  width: 11.875rem;
  text-indent: 0.3125rem;
  font-size: 0.8125rem;
  border: none;
  outline: none;
  border-bottom: 1px solid #7D7D7D;
}
.loginRegisterBox .loginRegister .verifyCode .verifyBtn {
  color: #333;
  text-decoration: underline;
  display: inline-block;
  width: 6.25rem;
  text-align: center;
  background-color: #B3CCFF;
}
.loginRegisterBox .loginRegister .passwordBox {
  height: 2.625rem;
  margin-bottom: 0.625rem;
}
.loginRegisterBox .loginRegister .passwordBox .passwordImg {
  width: 2.625rem;
  height: 2.625rem;
}
.loginRegisterBox .loginRegister .passwordBox .passwordText {
  width: 15.5rem;
  height: 2.625rem;
  line-height: 2.625rem;
  outline: none;
  border: 1px solid #999;
  text-indent: 0.3125rem;
}
.loginRegisterBox .loginRegister .loginBtn {
  font-size: 0.8125rem;
  color: #fff;
  width: 100%;
  padding: 0.5625rem 0;
  text-align: center;
  background-color: #FF4720;
}
