	<else/>
		<div class="li link_gray6 J_hoverbut t1 <if condition="ACTION_NAME eq 'jobs_add'">select</if>" onclick="window.location='{:U('company/jobs_add')}'"><a href="{:U('company/jobs_add')}"> Post position </a></div>
