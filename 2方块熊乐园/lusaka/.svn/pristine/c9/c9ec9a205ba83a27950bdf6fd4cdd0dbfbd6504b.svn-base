  <div class="li link_gray6 J_hoverbut t4 <if condition="$statistics_nav eq 'statistics_visitor'">select</if>" onclick="window.location='{:U('company/statistics_visitor')}'"><a href="{:U('company/statistics_visitor')}"> Recruitment results statistics </a></div>
