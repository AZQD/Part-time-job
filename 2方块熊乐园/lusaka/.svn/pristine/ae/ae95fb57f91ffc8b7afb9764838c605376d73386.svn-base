body{
    overflow: auto;
}

/*searchBox*/
.searchBox{
    min-width: 1200px;
    /*background-color: #FFCDC0;*/
}
.searchBox .search{
    width:1200px;
    height:100px;
    line-height:100px;
    margin:0 auto;
    font-size: 14px;
    color: #666;
    background-color: #FFCDC0;
    border-bottom: 2px solid #FF4720;
}
.searchBox .search .searchLeft{
    font-size: 13px;
}
.searchBox .search .searchLeft .fist{
    padding-left:16px;
    color: #333;
    text-decoration: underline;
}
.searchBox .search .searchLeft .gt{
    padding:0 12px;
}
.searchBox .search .searchLeft .second{
    color: #333;
}
.searchBox .search .searchLeft .second:after{
    display: inline-block;
    position: relative;
    top:4px;
    left:4px;
    content: "";
    font-size: 0;
    border-top: 6px solid #909090;
    border-bottom: 6px solid transparent;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
}
.searchBox .search .searchLeft .change{
    text-decoration: underline;
}
.searchBox .search .searchRight{
    text-align: right;
}
.searchBox .search .searchRight .ipt{
    width: 310px;
    height: 35px;
    line-height:35px;
    font-size: 13px;
    outline: none;
    text-indent: 10px;
    letter-spacing: 0;
    border: 1px solid #FF4720;
}
.searchBox .search .searchRight .searchImg{
    cursor: pointer;
    width:70px;
    margin-left:-6px;
}
.searchBox .search .searchRight .searchBtn{
    width: 127px;
    height: 35px;
    line-height:35px;
    color: #fff;
    font-size: 13px;
    border: none;
    outline: none;
    background-color: #FF4720;
    margin-right:42px;
    position: relative;
    top:1px;
}

.searchBox .search .menuUl{
    list-style: none;
    cursor: pointer;
    position: absolute;
    z-index: 1000;
    top:66px;
    padding:0;
    background-color: #fff;
    border: 1px solid #999;
}
.searchBox .search .menuUl .menuLi{
    border-bottom: 1px solid #999;
    width: 97px;
    height: 29px;
    line-height:29px;
    font-size: 13px;
    text-align: center;
    position: relative;
}
.searchBox .search .menuUl .menuLi .menuLiLink{
    color: #333333;
}
.searchBox .search .menuUl .menuLi:hover{
    background-color: #F0F0F0;
}
.searchBox .search .menuUl .menuLi:last-child{
    border-bottom: 0;
}
.searchBox .search .menuUl .menuLi .twoUl{
    background-color: #fff;
    border: 1px solid #999;
    position: absolute;
    left:97px;
    top:-1px;
}
.searchBox .search .menuUl .menuLi .twoUl .twoLi{
    border-bottom: 1px solid #999;
    width: 97px;
    height: 29px;
    line-height:29px;
    font-size: 13px;
    text-align: center;
}
.searchBox .search .menuUl .menuLi .twoUl .twoLi:last-child{
    border-bottom: 0;
}
.searchBox .search .menuUl .menuLi .twoUl .twoLi .twoLiLink{
    color: #333333;
}
.searchBox .search .menuUl .menuLi .twoUl .twoLi:hover{
    background-color: #F0F0F0;
}

