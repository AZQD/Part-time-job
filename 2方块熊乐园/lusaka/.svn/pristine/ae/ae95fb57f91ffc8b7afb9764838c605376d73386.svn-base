#footerIframe{
    min-width: 1200px;
    width: 100%;
    margin-bottom: -5px;
    background-color: #F2F2F2;
}

/*分享的btn*/
ul.share-buttons{
    list-style: none;
    padding: 0;
}

ul.share-buttons li{
    display: inline;
}

ul.share-buttons .sr-only {
    position: absolute;
    clip: rect(1px 1px 1px 1px);
    clip: rect(1px, 1px, 1px, 1px);
    padding: 0;
    border: 0;
    height: 1px;
    width: 1px;
    overflow: hidden;
}

ul.share-buttons img{
    width: 32px;
}

/*popBopx弹窗*/
.tipOffPopBox{
    position: absolute;
    background-color: RGBA(0,0,0,0.6);
    /*background-color: #000000;*/
    /*opacity: .6;*/
    z-index: 9999;
    width: 100%;
    height: 100%;
}
.tipOffPopBox .tipOffBox{
    width: 600px;
    /*height: 300px;*/
    background-color: #fff;
    margin: 0 auto;
    margin-top: 150px;
    padding: 30px 24px;
}
.tipOffPopBox .tipOffBox .title{
    font-size: 18px;
    font-weight: bold;
    color: #333;
}
.tipOffPopBox .tipOffBox #detail{
    vertical-align: top;
    width: 448px;
    height: 133px;
    resize: none;
    font-size: 13px;
    color: #000000;
    outline: none;
    border: 1px solid #666;
}
.tipOffPopBox .tipOffBox #content{
    width: 246px;
    height: 35px;
    line-height:35px;
    outline: none;
    border: 1px solid #666;
    text-indent: 4px;
}
.tipOffPopBox .tipOffBox #content::-webkit-input-placeholder{
    color: #ccc;
}
.tipOffPopBox .tipOffBox .tip{
    color: #FF4720;
}
.tipOffPopBox .tipOffBox .content{
    padding-top: 20px;
}
.tipOffPopBox .tipOffBox .submit{
    text-align: center;
    padding-top: 32px;
}
.tipOffPopBox .tipOffBox .submit .submitBtn{
    width: 127px;
    height: 35px;
    line-height:35px;
    border:none;
    outline: none;
    background-color: #FF4720;
    color: #fff;
    font-size: 13px;
}