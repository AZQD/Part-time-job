<include file="public:header_min" />
<div class="user_head_bg">
	<div class="user_head">
		<div class="logobox">
			<a href="__ROOT__/"><img src="<if condition="C('qscms_logo_home')">{:attach(C('qscms_logo_home'),'resource')}<else/>../public/images/logo.gif</if>" border="0"/></a>
		</div>
		<div class="logotxt">
			|&nbsp;&nbsp;
			<if condition="ACTION_NAME eq 'login'">
				 Member Login 
			<else/>
				<if condition="$utype eq 0"> Sign Up </if>
				<if condition="$utype eq 1">企业 Sign Up </if>
				<if condition="$utype eq 2">个人 Sign Up </if>
			</if>
		</div>
		<div class="reg">
			<if condition="ACTION_NAME eq 'login'">
				 No account yet ？ <a href="{:U('members/register')}" class="btn_blue J_hoverbut btn_inline">Register</a>
			<else/>
				 There is already an account ？ <a href="{:U('members/login')}" class="btn_blue J_hoverbut btn_inline"> log in immediately </a>
			</if>
		</div>
		<div class="clear"></div>
	</div>
</div>