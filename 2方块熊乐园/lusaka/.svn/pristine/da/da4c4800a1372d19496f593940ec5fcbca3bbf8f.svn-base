/*managePopBox弹窗*/
.managePopBox{
  position: absolute;
  background-color: RGBA(0,0,0,0.6);
  z-index: 999999;
  width: 100%;
  height: 100%;
}
.managePopBox .managePop{
  //width: 504/@rem;
  //height: 534/@rem;
  background-color: #fff;
  //margin: 70/@rem auto 0 auto;
  margin: 50/@rem auto 0 auto;
  padding: 12/@rem 16/@rem;
  position: relative;
}
.managePopBox .managePop .closePopImg{
  width: 1.25rem;
  position: absolute;
  right: 0;
  top: 0;
}

.managePopBox .managePop .fileBtn{
  position: relative;
}
.managePopBox .managePop .fileBtn .upload{
  /*float: left;*/
  width: 127/@rem;
  //height: 35/@rem;
  //line-height:35/@rem;
  padding: 8/@rem 0;
  margin-right: 16/@rem;
  font-size: 13/@rem;
  color: #fff;
  background-color: #FF4720;
  /*padding: 4/@rem 10/@rem;*/
  position: absolute;
  left: 0;
  /*bottom: 0;*/
  text-align: center;
  cursor: pointer;
  /*width: 100%;*/
  /*border-radius: 4/@rem;*/
  overflow: hidden;
  display: inline-block;
  *display: inline;
  *zoom: 1;
}
.managePopBox .managePop .fileBtn .upload input{
  position: absolute;
  font-size: 100/@rem;
  right: 0;
  top: 0;
  opacity: 0;
  filter: alpha(opacity=0);
  cursor: pointer;
}
.managePopBox .managePop .warning{
  padding-top: 40/@rem;
  font-size: 13/@rem;
  color: #999;
  padding-bottom: 5/@rem;
}
.managePopBox .managePop .imgBox{
  //height: 240/@rem;
  background-color: #F9F9F9;
  margin: 0 auto;
  border: 1px solid #E9EAEC;
  margin-top: 8/@rem;
  display: table-cell;
  vertical-align: middle;
  text-align: center;
  width: 100%;
  padding: 10/@rem 0;
}
.managePopBox .managePop .imgBox img{
  width: 120/@rem;
  height: 120/@rem;
}
.managePopBox .managePop .recommedTitle{
  font-size: 14/@rem;
  color: #666;
  padding-top: 18/@rem;
  padding-bottom: 8/@rem;
}
