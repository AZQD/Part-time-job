<notempty name="jobs_list">
	<volist name="jobs_list" id="jobs">
		<div class="list_cell_box">
			<div class="td1">
				<div class="jobname link_blue substring"><a target="_blank" href="{$jobs.jobs_url}" title="{$jobs.jobs_name}">{$jobs.jobs_name}</a></div>
				<div class="edu_wage substring">
					<div class="education"> experience {$jobs.experience_cn} / {$jobs.education_cn}</div>
					<div class="wage font_yellow">{$jobs.wage_cn}</div>
					<div class="clear"></div>
				</div>
				<div class="cname link_gray9 substring"><a target="_blank" href="{:url_rewrite('QS_companyshow',array('id'=>$jobs['company_id']))}" title="{$jobs.companyname}">{$jobs.companyname}</a></div>
			</div>
		</div>
	</volist>
	<div class="clear"></div>
<else />
	<div class="empty_tipstxt link_blue"> Did not find the relevant position ， you can <a target="_blank" href="{:url_rewrite('QS_jobslist')}" target="_blank"> Go to the post library </a> Try to cheer ！</div>
</notempty>