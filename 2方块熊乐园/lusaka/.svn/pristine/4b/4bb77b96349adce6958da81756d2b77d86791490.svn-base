}
.likeBox .like .likeTitle .gang {
  color: #FF4720;
  padding-left: 0.3125rem;
}
.likeBox .like .likeHeight {
  height: 13.125rem;
  overflow: hidden;
}
.likeBox .like .likeList .likeLink {
  display: inline-block;
  position: relative;
  margin-bottom: 0.625rem;
  text-align: center;
}
.likeBox .like .likeList .likeLink .goodImgWrap {
  display: inline-block;
  width: 4.8125rem;
  height: 4.8125rem;
}
.likeBox .like .likeList .likeLink .goodImg {
  width: 4.8125rem;
  height: 4.8125rem;
  border: 1px solid #FF4720;
}
.likeBox .like .likeList .likeLink .desc {
  display: none;
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 0;
  padding: 0 0.375rem;
  height: 1.875rem;
  line-height: 1.875rem;
  font-size: 1rem;
  color: #fff;
  background-color: #FF4720;
}
.likeBox .like .likeList .likeLink .left {
  font-weight: bold;
}
.likeBox .like .likeList .likeLink .right {
  text-align: right;
}
.likeBox .like .more {
  background-color: #E6E6E6;
  font-size: 0.8125rem;
  text-align: center;
  padding: 0.125rem 0;
}
.likeBox .like .more .moreText {
  color: #333333;
}
.likeBox .like .more .moreText:before {
  display: inline-block;
  position: relative;
  top: 0.3125rem;
  left: -0.25rem;
  content: "";
  font-size: 0;
  border-top: 0.4375rem solid #333333;
  border-bottom: 0.4375rem solid transparent;
  border-left: 0.4375rem solid transparent;
  border-right: 0.4375rem solid transparent;
}
/*messageBox*/
.messageBox {
  margin-bottom: 3.75rem;
}
.messageBox .message {
  margin: 0 auto;
  padding-bottom: 0.625rem;
  padding-left: 0.3125rem;
  padding-right: 0.3125rem;
}
.messageBox .message .messageTitle {
  font-size: 1.125rem;
  color: #333;
  font-weight: bold;
  margin: 1rem 0;
}
.messageBox .message .messageTitle .gang {
  color: #FF4720;
}
/*infos*/
.infos {
  position: fixed;
  width: 100%;
  left: 0;
  bottom: 0;
  box-shadow: 0 0 1.25rem #333333;
}
.infos .part {
  font-size: 0.875rem;
  padding: 0.625rem 0;
  color: #fff;
  text-align: center;
  background-color: #FF4720;
  font-weight: bold;
}
.infos .profilePublish {
  background: #ff4720 url("../image/index/borderWhite.png") no-repeat right top;
  background-size: 1px 100%;
}
.infos .goToHome {
  background: #ff4720 url("../image/index/borderWhite.png") no-repeat left top;
  background-size: 1px 100%;
}
.infos .part span {
  text-decoration: underline;
}
