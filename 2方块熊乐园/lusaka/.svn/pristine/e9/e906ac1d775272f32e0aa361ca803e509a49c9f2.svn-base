/*a  upload */
.a-upload {
  border: 1px solid #999;
  float: left;
  margin-right: 6/@rem;
  width: 145/@rem;
  /*padding: 4/@rem 10/@rem;*/
  height: 145/@rem;
  //line-height: 145/@rem;
  position: relative;
  cursor: pointer;
  color: #888;
  /*border-radius: 4/@rem;*/
  overflow: hidden;
  display: inline-block;
  *display: inline;
  *zoom: 1;
  margin-bottom: 20/@rem;
}
.a-upload .closeImg{
  width: 20/@rem;
  position: absolute;
  right: 0;
  top: 0;
}

.a-upload input {
  position: absolute;
  font-size: 100/@rem;
  right: 0;
  top: 0;
  opacity: 0;
  filter: alpha(opacity=0);
  cursor: pointer;
  z-index: 999999;
}


/*publishPopBox弹窗*/
.publishPopBox{
