<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Center ad</title>
    
    <meta name="description" content="online game, html game, h5 game, iOS game, android game">
    <meta name="author" content="youkongwan.com">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    
    
    <style>
    	html,
		body {
			margin: 0;
			padding: 0;
			border: 0;
		
			width: 100%;
			height: 100%;
		
			scroll:no;
		
			font-family: Arial, Verdana;
		}
	
		.container {
			position: relative;
			width: 100%;
			height: 100%;
			background-color: #DADADA;
			overflow: hidden;
		}
	
	
		.closeImg {
			width:40px;
			height:40px;
			position:absolute;
			right:2px;
		}
	
		.ad {
			width: 100%;
			height: 100%;
			
			#border:1px solid #DADADA
		}
	
	
		.ad img {
			width:100%;
			height:100%;
		}
		
		
    </style>
    
    
    <script src="candyoyoad.js"></script>
    <!--script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.8.0.js"></script-->

    <script>
    	
    	(function() {
			var urlParameters = new Object();
			var url = location.search; //获取url中"?"符后的字串 
			if (url.indexOf("?") != -1) { 
				var str = url.substr(1); 
				strs = str.split("&"); 
				for(var i = 0; i < strs.length; i ++) { 
					urlParameters[strs[i].split("=")[0]] = unescape(strs[i].split("=")[1]); 
				} 
			}
			
			document.reviveZoneID = urlParameters["reviveZoneID"];
			document.reviveAdName = urlParameters["reviveAdName"];
		})();
		
  	</script>
  </head>
  <body id="main">

<div class="container" id="contentMain">
	<div id="adC_" class="ad" onclick="toOpen()">
		<script type='text/javascript'><!--//<![CDATA[
   var m3_u = (location.protocol=='https:'?'https://revive.youkongwan.cn/www/delivery/ajs.php':'http://revive.youkongwan.cn/www/delivery/ajs.php');
   var m3_r = Math.floor(Math.random()*99999999999);
   if (!document.MAX_used) document.MAX_used = ',';
   document.write ("<scr"+"ipt type='text/javascript' src='"+m3_u);
   document.write ("?zoneid=" + document.reviveZoneID);
   document.write ('&amp;cb=' + m3_r);
   if (document.MAX_used != ',') document.write ("&amp;exclude=" + document.MAX_used);
   document.write (document.charset ? '&amp;charset='+document.charset : (document.characterSet ? '&amp;charset='+document.characterSet : ''));
   document.write ("&amp;loc=" + escape(window.location));
   if (document.referrer) document.write ("&amp;referer=" + escape(document.referrer));
   if (document.context) document.write ("&context=" + escape(document.context));
   if (document.mmm_fo) document.write ("&amp;mmm_fo=1");
   document.write ("'><\/scr"+"ipt>");
//]]>-->
	</script>
	</div>
</div>
</body>

<script>	
	init(document.reviveAdName, null, null, null, null);
</script>
</html>
